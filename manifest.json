{
  "manifest_version": 3,
  "name": "GuitarsCanada Follow Link",
  "description": "Restores the missing Follow link on GuitarsCanada marketplace ads.",
  "version": "1.2.0",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.guitarscanada.com/threads/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}